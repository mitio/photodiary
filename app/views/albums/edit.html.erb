<% page_title = t('.edit_album') %>
<% content_for :page_title, page_title %>

<div class="albumTitle">
  <h1><%= page_title %></h1>
  <div class="albumLinks">
    <%= link_to t('.add_photo'), new_album_photo_path(@album) %> |
    <%= link_to t('preview'), album_photos_path(@album) %> |
    <%= link_to t('delete'), [current_user, @album], confirm: t('.delete_msg'), method: :delete %> |
  </div>
</div>

<%= render 'form' %>
